{
  "private": true,
  "name": "@said-m/instagram-cli",
  "version": "0.0.1",
  "description": "CLI tool for getting data from Instagram without API tokens",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Said Magomedov <saids.inbox@gmail.com>",
  "license": "MIT",
  "keywords": [
    "utils",
    "template",
    "TypeScript"
  ],
  "files": [
    "tslint.json",
    "tsconfig.json",
    "tsconfig.build.json",
    "jest.config.json",
    "dist",
    ".vscode"
  ],
  "bin": "dist/cli.js",
  "scripts": {
    "start": "ts-node src/cli.ts",
    "build": "tsc -p ./tsconfig.build.json",
    "validate": "tsc --noEmit",
    "lint": "tslint -p tsconfig.json",
    "prepack": "yarn build",
    "postpack": "rm -rf ./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "@said-m/common": "^0.0.1",
    "argparse": "^1.0.10",
    "request-promise": "^4.2.4"
  },
  "optionalDependencies": {
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/node": "^12.7.2",
    "@types/request-promise": "^4.1.44",
    "husky": "^3.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  }
}
